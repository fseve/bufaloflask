CREATE TABLE usuarios (
id INTEGER PRIMARY KEY AUTOINCREMENT,
correo TEXT(50) NOT NULL,
password TEXT NOT NULL,
nombres TEXT(100) NOT NULL,
apellidos TEXT(100) NOT NULL,
edad INTEGER NOT NULL,
idGenero INTEGER REFERENCES Generos(id) NOT NULL,
idCargo INTEGER REFERENCES Cargos(id) NOT NULL,
fechaIngreso TEXT(20) NOT NULL,
idTipoContrato INTEGER REFERENCES Cargos(id) NOT NULL,
fechaTerminoContrato TEXT(20) NOT NULL,
dependencia TEXT(100) NOT NULL,
salario REAL NOT NULL,
rol TEXT(20) NOT NULL
)

DROP TABLE USUARIOS

CREATE TABLE generos (
id INTEGER PRIMARY KEY AUTOINCREMENT,
descripcion TEXT(10) NOT NULL
)

INSERT INTO usuarios (correo, password, nombres, apellidos, edad, genero, cargo, fechaIngreso, tipoContrato, fechaTerminoContrato, dependencia, salario, rol) VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?)

SELECT * FROM usuarios

INSERT INTO generos (descripcion) VALUES ('fem')
SELECT * FROM generos
DELETE FROM generos


CREATE TABLE cargos (
id INTEGER PRIMARY KEY AUTOINCREMENT,
descripcion TEXT(100) NOT NULL
)

SELECT * FROM cargos
DELETE FROM cargos
CREATE TABLE tiposContratos (
id INTEGER PRIMARY KEY AUTOINCREMENT,
descripcion TEXT(100) NOT NULL
)


SELECT * FROM tiposContratos


CREATE TABLE dependencias (
id INTEGER PRIMARY KEY AUTOINCREMENT,
descripcion TEXT(100) NOT NULL
)

SELECT * FROM dependencias
